{
    "manifest_version": 2,
    "name": "YouTube pause with mouse",
    "version": "1.0.0",
    "content_scripts": [
        {
          "matches": [
            "https://www.youtube.com/*"
          ],
          "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_icon": "icon.png"
    }
}